<nav>
  <h1>GYSD</h1>
  <ul class="user_nav">
    <% if user_signed_in? %>
      <li><%= link_to 'Projects', root_path %></li>
    <% end %>
  </ul>

  <ul class="user_info">
    <% if user_signed_in? %>
      <li><%= link_to 'Edit account', edit_user_registration_path %></li>
